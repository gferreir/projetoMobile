<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gferreir.projectcleaner">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone_inicio"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icone_inicio"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".InicioActivity"/>
        <activity android:name=".GravaActivity"/>
        <activity android:name=".BuscaActivity"/>
        <activity android:name=".EdicaoActivity"/>
        <activity android:name=".RegistrarActivity"/>
    </application>

</manifest>